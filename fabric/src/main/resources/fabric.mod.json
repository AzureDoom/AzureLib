{
  "schemaVersion": 1,
  "id": "azurelib",
  "version": "${mod_version}",
  "name": "AzureLib",
  "description": "Based off Geckolib but now just for my own needs.",
  "authors": [
    "AzureDoom, Gecko, Eliot, Hydos, DerToaster, Tslat, KyoSleep, Bvanseg"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/azurelib",
    "issues": "https://github.com/AzureDoom/AzureLib/issues",
    "sources": "https://github.com/AzureDoom/AzureLib"
  },
  "license": "MIT",
  "icon": "azurelib.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "mod.azure.azurelib.fabric.FabricAzureLibMod"
    ],
    "client": [
      "mod.azure.azurelib.fabric.ClientListener"
    ],
    "modmenu": [
      "mod.azure.azurelib.fabric.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "azurelib.mixins.json",
    "azurelib.fabric.mixins.json"
  ],
  "accessWidener": "azurelib.aw",
  "depends": {
    "fabricloader": ">=0.14.10",
    "fabric-api": "*",
    "java": ">=17",
    "minecraft": ">=1.20.2"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  }
}